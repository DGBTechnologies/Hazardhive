{
	"cyInit": [
		
		{
			"selector": "node",
			"css": {
				"height": 59,
				"width": 48,
				"text-wrap": "ellipsis",
				"text-max-width": 200,
				"shape": "rectangle",
				"background-color": "white",
				"background-fit": "cover"
			}
		},
		{
			"selector": "node.searched",
			"style":{
				"border-color":"red",
				"border-width":5
			}
		},
		{
			"selector": "edge",
			"style": {
				"label":"data(label)",
				"curve-style": "bezier",
				"target-arrow-shape": "none",
				"width": 2,
				"line-color": "#00ACEE"
			}
		},
		{
			"selector": "edge.bezier",
			"style": {
				"curve-style": "taxi"
			}
			
		},
	
		
		{
			"selector": "node.hazard",
			"css": {
				"width": 130,
				"height": 60,
				"text-valign": "center",
				"text-halign": "center" ,
				"text-wrap": "ellipsis", 
				"text-max-width": "120px",
				"text-margin-y": "0.5em"
				,"label": "data(name)",
				"background-image": "../assets/images/hazard.png"
			}
		},


		{
			"selector": "node.threat",
			"style" : {
				"shape": "round-rectangle",
				"background-color": "#F6821d",
				"label": "data(name)",
				"text-wrap": "wrap", 
				"text-max-width": "120px",
				"text-valign": "center",
				"text-halign": "center",
				"width": 130,
				"height": 60
			}
		
		},
		{
			"selector": "node.consequence",
			"style" : {
				"shape": "round-rectangle",
				"background-color": "#ff625a",
				"label": "data(name)",
				"text-wrap": "wrap", 
				"text-max-width": "120px",
				"text-valign": "center",
				"text-halign": "center",
				"width": 130,
				"height": 60
			}
		
		},
		{
			"selector": "node.preventativeBarrier",
			"style" : {
				"shape": "round-rectangle",
				"background-color": "#e7e7e8",
				"label": "data(name)",
				"text-wrap": "wrap", 
				"text-max-width": "120px",
				"text-valign": "center",
				"text-halign": "center",
				"width": 130,
				"height": 60
			}
		
		},
		{
			"selector": "node.mitigativeBarrier",
			"style" : {
				"shape": "round-rectangle",
				"background-color": "#e7e7e8",
				"label": "data(name)",
				"text-wrap": "wrap", 
				"text-max-width": "120px",
				"text-valign": "center",
				"text-halign": "center",
				"width": 130,
				"height": 60
			}
		
		},
		{
			"selector": "node.preEscalation",
			"style" : {
				"shape": "round-rectangle",
				"background-color": "#f5b65c",
				"label": "data(name)",
				"text-wrap": "wrap", 
				"text-max-width": "120px",
				"text-valign": "center",
				"text-halign": "center",
				"width": 130,
				"height": 60
			}
		
		},
		{
			"selector": "node.postEscalation",
			"style" : {
				"shape": "round-rectangle",
				"background-color": "#f5b65c",
				"label": "data(name)",
				"text-wrap": "wrap", 
				"text-max-width": "120px",
				"text-valign": "center",
				"text-halign": "center",
				"width": 130,
				"height": 60
			}
		
		},
		{
			"selector": "node.top_event",
			"style":{"shape": "ellipse",
			"label": "data(name)",
			"background-color": "#4bc48c",
			"width": 130,
			"height": 130,
			"text-valign": "center",
			"text-halign": "center",
			"text-wrap": "ellipsis",
			"text-max-width": "120px"
		}
		
		},
		{
			"selector": "node.expandCollapse-node",
			"style": {
				"width": "20px",
				"height": "20px",
				"background-color": "#585858",
				"opacity": "0.50",
				"font-size":"30px" ,
				"text-valign": "center",
				"text-halign": "center",
				"shape": "round-rectangle",
				"content": "data(label)"
			}
		},
		
		{
			"selector": ":parent",
			"css": {
				"background-opacity": 0.33,
				"border-width": 3,
				"border-color": "#585858",
				"border-style": "dashed",
				"z-compound-depth": "auto"
			}
		}
		
	],
	".cy-popLabel" : {
		"width": "145px",
		"padding":"5px",
		"font-size": "12px",
		"text-align": "left",
		"border-radius": "1px",
		"border":" 0.00014px solid grey",
		"background":" #fff",
		"text-overflow": "auto",
		"overflow-wrap": "break-word",
		"overflow-x": "auto"
	},
	"cyPanZoom": {
		"zoomFactor": 0.05,
		"zoomDelay": 45,
		"minZoom": 0.1,
		"maxZoom": 10,
		"fitPadding": 50,
		"panSpeed": 10,
		"panDistance": 10,
		"panDragAreaSize": 75,
		"panMinPercentSpeed": 0.25,
		"panInactiveArea": 8,
		"panIndicatorMinOpacity": 0.5,
		"zoomOnly": false,
		"fitSelector": "undefined",
		"fitAnimationDuration": 1000,
		"zoomInIcon": "fa fa-plus-circle",
		"zoomOutIcon": "fa fa-minus-circle",
		"resetIcon": "fa fa-refresh"
	},
	"cyNavigator": {
		"container": ".cytoscape-navigator",
		"viewLiveFramerate": 0,
		"thumbnailEventFramerate": 30,
		"thumbnailLiveFramerate": false,
		"dblClickDelay": 200,
		"removeCustomContainer": true,
		"rerenderDelay": 100
	},
	"cyGrid": {
		"snapToGridOnRelease": true,
		"snapToGridDuringDrag": false,
		"snapToAlignmentLocationOnRelease": false,
		"snapToAlignmentLocationDuringDrag": false,
		"distributionGuidelines": false,
		"geometricGuideline": false,
		"initPosAlignment": false,
		"centerToEdgeAlignment": false,
		"resize": false,
		"parentPadding": false,
		"drawGrid": true,
		"gridSpacing": 15,
		"snapToGridCenter": true,
		"zoomDash": true,
		"panGrid": false,
		"gridStackOrder": -1,
		"gridColor": "#dedede",
		"lineWidth": 1.0,
		"guidelinesStackOrder": 4,
		"guidelinesTolerance": 2.00,
		"guidelinesStyle": {
			"strokeStyle": "#8b7d6b",
			"geometricGuidelineRange": 400,
			"range": 100,
			"minDistRange": 10,
			"distGuidelineOffset": 10,
			"horizontalDistColor": "#ff0000",
			"verticalDistColor": "#00ff00",
			"initPosAlignmentColor": "#0000ff",
			"lineDash": [
				0,
				0
			],
			"horizontalDistLine": [
				0,
				0
			],
			"verticalDistLine": [
				0,
				0
			],
			"initPosAlignmentLine": [
				0,
				0
			]
		},
		"parentSpacing": -1
	}
}